<section class="container">
  <h1>New Project</h1>
  <form method="post" action="/projects">
  <div class="form-group">
    <label for="inputTitleMain">Title</label>
    <input type="text" class="form-control" id="inputTitleMain" placeholder="Title" name="project[titles][main]">
  </div>
  <div class="form-group">
    <label for="creator">Creator</label>
    <input type="text" class="form-control" id="creator" placeholder="Creator" name="project[creator]">
  </div>
  <div class="form-group">
    <label for="inputColorsTopLeft">Gradient Start</label>
    <input type="text" class="form-control" id="inputColorsTopLeft" placeholder="150, 150, 150" name="project[colors][topLeft]">
  </div>
  <div class="form-group">
    <label for="inputColorsBottomRight">Gradient End</label>
    <input type="text" class="form-control" id="inputColorsBottomRight" placeholder="100, 100, 100" name="project[colors][bottomRight]">
  </div>
  <div class="form-group">
    <label for="imageUrlsBanner">Banner Image URL</label>
    <input type="text" class="form-control" id="imageUrlsBanner" placeholder="Banner Image URL" name="project[imageUrls][banner]">
  </div>
  <div class="form-group">
    <label for="paragraphsIntro">Introduction</label>
    <textarea type="text" class="form-control" id="paragraphsIntro" name="project[paragraphs][intro]"></textarea>
  </div>
  <div class="form-group">
    <label for="paragraphsmain">Main Paragraph</label>
    <textarea type="text" class="form-control" id="paragraphsMain" name="project[paragraphs][main]"></textarea>
  </div>
  <div class="form-group">
    <label for="imageUrlsIntro">Intro Image URL</label>
    <input type="text" class="form-control" id="imageUrlsIntro" placeholder="Intro Image URL" name="project[imageUrls][intro]">
  </div>
  <div class="form-group">
    <label for="imageUrlsMain">Main Image URL</label>
    <input type="text" class="form-control" id="imageUrlsMain" placeholder="Main Image URL" name="project[imageUrls][main]">
  </div>
  <div class="form-group">
    <label for="imageUrlsConcluding">Concluding Image URL</label>
    <input type="text" class="form-control" id="imageUrlsConcluding" placeholder="Concluding Image URL" name="project[imageUrls][concluding]">
  </div>
  <div class="form-group">
    <label for="paragraphsConcluding">Concluding Paragraph</label>
    <textarea class="form-control" id="paragraphsConcluding" name="project[paragraphs][concluding]"></textarea>
  </div>
  <button type="submit" class="btn btn-default">Create</button>
</form>
</section>
