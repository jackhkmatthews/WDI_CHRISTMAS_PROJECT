<h1><%= project.titles[0].main %></h1>
<h3><%= project.creator %></h3>
</div>
</div>
<section class="container">
  <div class="row">
    <div class="col-sm-6 col-sm-offset-3">
      <div class="page-header">
        <h1><%= project.paragraphs[0].intro %></h1>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-10 col-sm-offset-1">
      <p class="paragraph-main">
        <%= project.paragraphs[0].main %>
      </p>
    </div>
  </div>
  <div class="image-contianer">
    <img src="<%= project.imageUrls[0].intro %>" class="img-responsive" alt="">
  </div>
  <div class="image-contianer">
    <img src="<%= project.imageUrls[0].main %>" class="img-responsive" alt="">
  </div>
  <div class="image-contianer">
    <img src="<%= project.imageUrls[0].concluding %>" class="img-responsive" alt="">
  </div>
  <div class="row">
    <div class="col-sm-10 col-sm-offset-1">
      <p class="paragraph-main">
        <%= project.paragraphs[0].concluding %>
      </p>
    </div>
  </div>
  <a class="btn btn-default" href="/projects/<%= project._id %>/edit" role="button">Edit</a>
  <form action="/projects/<%= project._id %>" method="post">
    <input type="hidden" name="_method" value="delete">
    <button type="submit" class="btn btn-danger">Delete</button>
  </form>
</section>
